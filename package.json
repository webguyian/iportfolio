  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config config/webpack.config.babel.js",
    "eslint:base": "eslint --ignore-path .gitignore",
    "format:prettier": "npm run prettier:base -- --write \"**/*.{js,jsx}\"",
    "format:eslint": "npm run eslint:base -- --fix \"**/*.{js,jsx}\"",
    "lint": "eslint src/ --ext .jsx,.js --fix",
    "prettier:base": "prettier --ignore-path .gitignore",
    "start": "cross-env NODE_ENV=debug webpack-dev-server --config config/webpack.config.babel.js --key config/cert/key.pem --cert config/cert/cert.pem",
    "stylelint": "stylelint src/**/**/*.scss --fix",
    "test": "jest --coverage --watchAll",
    "test:all": "jest --bail --runInBand --coverage=false",
    "format": "npm run format:eslint && npm run format:prettier"
  },
  "keywords": [],
  "author": "Ian McPhail",
  "license": "ISC",
