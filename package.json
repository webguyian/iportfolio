{
  "name": "react-redux-template",
  "version": "1.0.0",
  "description": "A boilerplate template for creating React-Redux applications",
  "main": "./public/dist/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p --config config/webpack.config.babel.js",
    "start": "cross-env NODE_ENV=debug webpack-dev-server --config config/webpack.config.babel.js",
    "stylelint": "stylelint src/**/**/*.scss --fix",
    "test": "jest --coverage --watchAll"
  },
  "keywords": [
    "react",
    "babel",
    "webpack",
    "jest"
  ],
  "author": "Ian McPhail",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@babel/runtime": "7.5.2",
    "classnames": "2.2.6",
    "lodash": "^4.17.15",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.12.5",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-regenerator": "7.4.5",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/preset-env": "7.5.2",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "autoprefixer": "9.6.1",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "2.0.1",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-jest": "22.15.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "install-peers": "1.0.3",
    "jest": "24.8.0",
    "lint-staged": "8.1.5",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.12.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.17.0",
    "prettier-eslint": "8.8.2",
    "react-test-renderer": "16.8.6",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "10.1.0",
    "stylelint-a11y": "1.2.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-declaration-use-variable": "1.7.0",
    "stylelint-order": "3.0.0",
    "stylelint-webpack-plugin": "0.10.5",
    "terser-webpack-plugin": "1.3.0",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --bail --changedSince=master --coverage=false"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "prettier --single-quote --write",
        "git add"
      ],
      "*.scss:": [
        "stylelint --fix",
        "prettier --write",
        "git add"
      ],
      "*.{json}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "*.json",
      "*.test.js"
    ]
  }
}
