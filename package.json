  "browserslist": [
    "defaults",
    "IE >= 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --bail --changedSince=main --coverage=false"
    }
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "npm run eslint:base -- --fix --max-warnings 0",
      "npm run prettier:base -- --write"
    ],
    "*.scss": "stylelint --fix"
  }
